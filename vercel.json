{
    "redirects": [
      {
        "source": "/www.taylorconsulting.io/(.*)",
        "destination": "https://taylorconsulting.io/$1",
        "permanent": true
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }