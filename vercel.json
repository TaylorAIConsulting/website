{
    "redirects": [
      {
        "source": "/(.*)",
        "has": [{ "host": "www.taylorconsulting.io" }],
        "destination": "https://taylorconsulting.io/$1",
        "permanent": true
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }